<header class="header d-flex justify-content-center" id="gamesTitle">
    <a href='/games'>
        <h1 class="title" id="gamesTitleH1">Landing Page</h1>
    </a>
</header>
<div class="nav">
    <a href='/profile/{{id}}'><button id="profileBtn" class="btn gamesPageBtns">Profile</button></a>
    <button id="logoutButton" class="btn gamesPageBtns">Log Out</button>
</div>
<div class="rankBoard">
<ul>
    <h2>PLAYER: {{user_name}}</h2>
    <h2>Rank: 
        {{#each rankings as |rankings|}}
            <h2 class="ranksPage-rank">Game: {{rankings.game_id}}</h2>
            <h2 class="ranksPage-rank">Score: {{rankings.rank}}</h2>
        {{/each}}
    </h2>
</ul>
</div>
<script src="/js/logout.js"></script>
<script src="/js/profile.js"></script>