<header class="header">
    <h1 class="title">Landing Page Title</h1>
</header>
<div class="profilePage">
  <div class="profileTitle">
    <h1>Welcome, {{user_name}}</h1>
    <h2>Your Profile:</h2>
  </div>
  <div class="profileEdit">
    <form action="" id="newProfile" data-userID={{id}}>
      <input type="text" id="user_name" placeholder="user_name">
      <img id="image_url" src={{image_url}}>
      <input type='file' id="uploadfile" aria-label="File browser"/>
      <button class="btn btn-primary profileBtns">Submit</button>
    </form>
  </div>
  <form>
    <h3>{{user_name}}</h3>
    <img src={{image_url}}>
    <button class="btn btn-primary profileBtns">Delete</button>
  </form>
</div>
<script src="/js/profile.js"></script>
