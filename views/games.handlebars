<header class="header" id="gamesTitle">
  <a href="/games">
    <h1 class="title" id="gamesTitleH1">Landing Page Title</h1>
  </a>
</header>
<div class="nav">
  <a href="/ranks"><button
      id="ranks"
      class="btn gamesPageBtns"
    >Ranks</button></a>
  <a href="/profile/{{id}}"><button
      id="profileBtn"
      class="btn gamesPageBtns"
    >Profile</button></a>
  <button id="logoutButton" class="btn gamesPageBtns">Log Out</button>
</div>
<div class="games">
  <a
    href="/t3"
    id="t3Btn"
    class="gameBtns"
    data-gameID="1"
    data-userID={{id}}
  >Game 1</a>
  <a
    href="/c4"
    id="c4Btn"
    class="gameBtns"
    data-gameID="2"
    data-userID={{id}}
  >Game 2</a>
  <aside class="friends">
    <h3 class="friends-title">Friends list</h3>
    <ul>
      {{#each Friend as |friend|}}
        <li>{{friend.user_name}}</li>
      {{/each}}
    </ul>
  </aside>
</div>
<script src="/js/logout.js"></script>
<script src="/js/profile.js"></script>
